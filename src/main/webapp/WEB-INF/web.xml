<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Imagement</display-name>
  <listener>
    <listener-class>com.yauhenav.web.SessionListener</listener-class>
  </listener>

  <servlet>
    <display-name>Imagement</display-name>
    <servlet-name>Imagement</servlet-name>
    <servlet-class>com.yauhenav.web.Imagement</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Imagement</servlet-name>
    <url-pattern>/modify</url-pattern>
  </servlet-mapping>

  <servlet>
    <display-name>LoginServlet</display-name>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.yauhenav.web.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>

  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>  
    
</web-app>
